{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "standard"],
  "parser": "babel-eslint",
  "parserOptions": {
    "es6": true,
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "react/prop-types": "off",
    "no-extra-semi": "off",
    "camelcase": "off",
    "quotes": "off",
    "semi": "off",
    "indent": "off",
    "multiline-ternary": "off",
    "space-before-function-paren": "off",
    "no-useless-catch": "off",
    "comma-dangle": "off"
  }
}
